{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "NyaScript",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#methods"
		},
		{
			"include": "#moveCursor"
		},
		{
			"include": "#temp"
		},
		{
			"include": "#functions"
		},
		{
			"include": "#headers"
		},
		{
			"include": "#variables"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#conditions"
		},
		{
			"include": "#functions"
		},
		{
			"include": "#string"
		},
		{
			"include": "#classes"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.nyascript",
				"match": "\\bNYa\\b|\\bnYA\\b"
			}]
		},
		"methods": {
			"patterns": [{
				"match": "\\bNYA\\+\\+|\\bnya\\+\\+|\\bNYA\\+|\\bnya\\+|\\bNYA\\b|\\bnya\\b|\\bNya\\?|\\?Nya\\b",
				"name":"keyword"
			}]
		},
		"moveCursor": {
			"patterns": [{
				"match": "\\bNya|nyA\\b",
				"name":"keyword.control"
			}]
		},
		"temp": {
			"patterns": [{
				"match": "\\bNYAN|nyan|NyAN\\b",
				"name":"entity.name.type"
			}]
		},
		"headers": {
			"patterns": [{
				"match": "\\bOwO|UwU\\b",
				"name": "markup.bold"
			}]
		},
		"variables": {
			"patterns": [{
				"match": "\\#\\w*|\\$\\w*",
				"name":"variable.name"
			}]
		},
		"numbers": {
			"patterns": [{
				"match": "\\d",
				"name":"constant.numeric"
			}]
		},
		"conditions": {
			"patterns": [{
				"match": "\\bgweater\\b|\\blower\\b|\\beqwal\\b|\\bdiwides\\b",
				"name": "entity.other.attribute-name"
			}]
		},
		"functions": {
			"patterns": [{
				"match": "\\b(meow|meOW|MEOW|MEow)\\b",
				"name":"support.function"
			}]
		},
		"strings": {
			"patterns": [{
				"match": "\\b\"\\w*\"\\b",
				"name":"string"
			}]
		},
		"classes": {
			"patterns": [{
				"match": "\\bCwass\\b|\\bextends\\b|\\bupper\\b",
				"name":"support.class"
			},{
				"match": "->\\w*|#->\\w*|\\|->\\w*|\\sÂµ\\s",
				"name":"variable.parameter"
			},{
				"match": "/\\w*",
				"name":"entity.name.function"
			}]
		}
	},
	"scopeName": "source.nyascript"
}