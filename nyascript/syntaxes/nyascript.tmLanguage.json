{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "NyaScript",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#methods"
		},
		{
			"include": "#moveCursor"
		},
		{
			"include": "#temp"
		},
		{
			"include": "#functions"
		},
		{
			"include": "#headers"
		},
		{
			"include": "#variables"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#conditions"
		},
		{
			"include": "#functions"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.nyascript",
				"match": "\\bNYa|nYA\\b"
			}]
		},
		"methods": {
			"patterns": [{
				"match": "\\bNYA|nya|Nya\\?|\\?Nya\\b",
				"name":"keyword"
			}]
		},
		"moveCursor": {
			"patterns": [{
				"match": "\\bNya|nyA\\b",
				"name":"keyword.control"
			}]
		},
		"temp": {
			"patterns": [{
				"match": "\\bNYAN|nyan|NyAN\\b",
				"name":"entity.name.type"
			}]
		},
		"headers": {
			"patterns": [{
				"match": "\\bOwO|UwU\\b",
				"name": "markup.bold"
			}]
		},
		"variables": {
			"patterns": [{
				"match": "\\b#\\w*|\\$\\w*\\b",
				"name":"variable.name"
			}]
		},
		"numbers": {
			"patterns": [{
				"match": "\\d",
				"name":"constant.numeric"
			}]
		},
		"conditions": {
			"patterns": [{
				"match": "\\bgweater\\b|\\blower\\b|\\beqwal\\b",
				"name": "entity.other.attribute-name"
			}]
		},
		"functions": {
			"patterns": [{
				"match": "(\\s|^)(meow|meOW|MEOW)",
				"name":"support.function"
			}]
		}
	},
	"scopeName": "source.nyascript"
}